{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rulesDirectory": ["codelyzer"],
  "rules": {
    "interface-name": false,
    "array-type": false,
    "object-literal-sort-keys": false,
    "directive-selector": [true, "attribute", "cinder", "camelCase"],
    "component-selector": [true, "element", "cinder", "kebab-case"],
    "no-empty": false
  }
}
